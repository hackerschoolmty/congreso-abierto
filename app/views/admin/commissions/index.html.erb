<p id="notice"><%= notice %></p>

<h1>Listing Commissions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Commission Topic</th>
      <th>Legislature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @commissions.each do |commission| %>
      <tr>
        <td><%= commission.name %></td>
        <td><%= commission.commission_topic.name %></td>
        <td><%= commission.legislature.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Commission', new_admin_commission_path %>